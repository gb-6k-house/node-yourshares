{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "inlineSources": true,
    "inlineSourceMap": true,
    "outDir": "lib",
    "types": [],
    "typeRoots": [],
    "declaration": true,
    "declarationDir": "./lib/types", //定义声明ts声明文件的输出目录
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false
    // "noImplicitAny": false
  },
  "exclude": [
    "node_modules", "node_modules/grpc"
  ],
  "include": [
    "./src/**/*"
  ]
}

